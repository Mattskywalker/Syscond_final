<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<FlowPane fx:id="mainPane" alignment="TOP_CENTER" orientation="VERTICAL" prefHeight="470.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProcurarMoradorController">
    <children>
        <FlowPane alignment="CENTER" prefHeight="40.0" prefWidth="200.0">
            <children>
                <Label prefHeight="35.0" prefWidth="204.0" text="Procurar Morador">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Label>
            </children>
            <FlowPane.margin>
                <Insets />
            </FlowPane.margin>
        </FlowPane>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="CPF">
            <font>
                <Font size="18.0" />
            </font>
            <FlowPane.margin>
                <Insets top="10.0" />
            </FlowPane.margin>
        </Text>
        <TextField fx:id="textFieldCpf" prefHeight="27.0" prefWidth="247.0" style="-fx-border-color: #000; -fx-border-radius: 8; -fx-background-radius: 8;">
            <FlowPane.margin>
                <Insets top="5.0" />
            </FlowPane.margin>
        </TextField>
        <JFXButton fx:id="procurarMorador" alignment="CENTER" onAction="#procurarMorador" prefHeight="29.0" prefWidth="116.0" style="-fx-border-color: #000; -fx-border-radius: 8;" text="Pesquisar">
            <graphic>
                <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/magnifying-glass.png" />
                    </image>
                </ImageView>
            </graphic>
            <FlowPane.margin>
                <Insets top="5.0" />
            </FlowPane.margin>
        </JFXButton>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Dados encontrados:">
            <font>
                <Font size="18.0" />
            </font>
            <FlowPane.margin>
                <Insets top="5.0" />
            </FlowPane.margin>
        </Text>
        <TableView fx:id="tableView" prefHeight="67.0" prefWidth="566.0">
            <columns>
                <TableColumn fx:id="nomeCollumn" prefWidth="126.0" text="Nome" />
                <TableColumn fx:id="apartamentoColumn" prefWidth="136.0" text="Apartamento" />
                <TableColumn fx:id="carroColumn" prefWidth="141.0" text="Carros" />
            <TableColumn fx:id="visitanteColumn" prefWidth="162.0" text="Visitantes" />
            </columns>
            <FlowPane.margin>
                <Insets />
            </FlowPane.margin>
        </TableView>
        <Text strokeType="OUTSIDE" strokeWidth="0.0">
            <font>
                <Font size="18.0" />
            </font>
            <FlowPane.margin>
                <Insets top="5.0" />
            </FlowPane.margin>
        </Text>
        <Pane prefHeight="44.0" prefWidth="566.0">
            <children>
                <JFXButton fx:id="voltar" layoutX="466.0" layoutY="2.0" onAction="#voltar" prefHeight="40.0" prefWidth="100.0" style="-fx-border-color: #000; -fx-border-radius: 8;" text="Voltar" />
                <JFXButton fx:id="deletarMorador" alignment="CENTER" disable="true" layoutX="7.0" layoutY="1.0" onAction="#deletarMorador" prefHeight="29.0" prefWidth="116.0" style="-fx-border-color: #000; -fx-border-radius: 8;" text="Deletar">
                    <graphic>
                        <ImageView disable="true" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../img/mainMenu/remove.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </JFXButton>
            </children>
            <FlowPane.margin>
                <Insets />
            </FlowPane.margin>
        </Pane>
    </children>
</FlowPane>
